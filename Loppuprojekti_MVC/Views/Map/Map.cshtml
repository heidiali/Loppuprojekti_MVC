
@{
    ViewData["Title"] = "Map";
}

<!DOCTYPE html>
<html>
<head>
    <style>
        #map {
            height: 300px;
            width: 100%;
        }
    </style>
</head>
<body>
    <h3>Google Maps perseilyä</h3>
    <div id="map"></div>
    <script>
        function initMap() {
            var country = { lat: 60, lng: 60 };
            var map = new google.maps.Map(
                document.getElementById('map'), { zoom:4, center: country });
            var marker = new google.maps.Marker({center: country, map: map });
        }
    </script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=xxx">
    </script>
</body>

